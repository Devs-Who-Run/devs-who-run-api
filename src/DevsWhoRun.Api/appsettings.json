{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Database": {
    "ConnectionString": "Server=localhost;Port=5432;Database=devswhorun;User Id=postgres;Password=admin;",
    "CommandTimeout": 30,
    "MaxRetryCount": 3
  },
  "Swagger": {
    "Title": "Devs Who Run API",
    "Description": "API for managing the Devs Who Run community",
    "Version": "v1"
  },
  "Jwt": {
    "Key": "your-super-secret-key-with-at-least-32-characters",
    "Issuer": "devs-who-run-api",
    "Audience": "devs-who-run-client"
  },
  "GitHub": {
    "ClientId": "replace with the client id",
    "ClientSecret": "replace with the client secret",
    "AuthorizationEndpoint": "https://github.com/login/oauth/authorize",
    "TokenEndpoint": "https://github.com/login/oauth/access_token",
    "UserInformationEndpoint": "https://api.github.com/user",
    "Scope": "user:email",
    "RedirectUri": "http://localhost:5299/api/auth/github-callback"
  },
  "Discord": {
    "BotToken": "replace with the bot token",
    "ServerId": "replace with the server id"
  }
}
